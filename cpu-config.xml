# A config for a CPU-only folder, if you happen to find a great deal on a non-GPU VM

<config>
  <!-- Client Control -->
  <fold-anon v='false'/>

  <!-- Folding Slot Configuration -->
  <gpu v='false'/>

  <!-- Slot Control -->
  <power v='full'/>

  <!-- User Information -->
  <passkey v=''/>
  <team v='236388'/>
  <user v='texmandie'/>

  <!-- Folding Slots -->
  <slot id='0' type='CPU'>
    <client-type v='beta'/>
  </slot>
</config>